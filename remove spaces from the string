import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String n=sc.nextLine();
       String res="";
       
        for(int i=0;i<n.length();i++){
            char c=n.charAt(i);
        
        if(c!=' '){
            res+=c;
        }
        }
        
        System.out.println(res);
    }
}
